<%= form_with model: CartItem.new, url: cart_items_path, method: :post, local: true do |f| %>
  <%= f.hidden_field :product_id, value: product_id %>
  <%= f.hidden_field :product_type, value: product_type %>
  <%= f.hidden_field :license_id, value: license_id %>
  <%= f.hidden_field :quantity, value: quantity %>

  <%= ui %>
<% end %>
